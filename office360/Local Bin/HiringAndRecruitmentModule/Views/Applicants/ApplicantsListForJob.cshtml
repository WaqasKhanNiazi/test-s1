@using System.Web.Mvc.Html
@{
    ViewBag.Title = "Applicant List";
    Layout = "~/Views/Shared/_LayOut.cshtml";
}
@using (Html.BeginForm())
{

    <div class="row">
        <div class="col-md-12 offset-lg-0">
                @*<legend style="color: navy; margin-left: 10px">Applicant List</legend>*@
                <div class="bootstrap-admin-panel-content form-group">
                    <table class="table table-striped table-bordered order-column stripe table-sm table-hover" style="width: 100%" id="TblApplicantList">
                        <tbody id="tableBody">
                        </tbody>
                    </table>
                </div>
        </div>
    </div>
    <input type="text" hidden id="HiddenFieldAdvertismentCode" value="@ViewBag.AdvertismentCode" />
}
<script src="~/Areas/HiringAndRecruitmentModule/JsScript/ApplicantsListForJob.js"></script>
