@using System.Web.Mvc.Html
@{
    ViewBag.Title = "Inter-View List";
    Layout = "~/Views/Shared/_LayOut.cshtml";
}
@using (Html.BeginForm())
{
    <div class="row">
        <div class="col-md-12 offset-lg-0">
            @*<legend style="color: navy; margin-left: 10px">Applicant List</legend>*@
            <div class="bootstrap-admin-panel-content form-group">
                <table class="table table-striped table-bordered order-column stripe table-sm table-hover" style="width: 100%" id="TblInterviewList">
                    <tbody id="tableBody">
                    </tbody>
                </table>
            </div>
        </div>
    </div>
    <div id="DivDecision" style="display:none">
    <div class="m-t-20 text-center btn btn-primary">
        <i class="fa fa-save btn"></i><span>
            <input type="button" class="btn btn-primary" id="SubmitDownApproveHiring" value="Approve" />
        </span>
    </div>
    <div class="m-t-20 text-center btn btn-danger">
        <i class="fa fa-save btn"></i><span>
            <input type="button" class="btn btn-danger" id="SubmitDownRejectHiring" value="Reject" />
        </span>
    </div>

 </div>

}
<script src="~/Areas/HiringAndRecruitmentModule/JsScript/InterViewList.js"></script>
